#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int T,n,i,j,cnt;
        scanf("%d",&T);
        for(i=0;i<T;i++)
        {
                j=1;     
                cnt=0;
                scanf("%d",&n);
                while(n>0)
                {
                 if(n>=j)
                 {
                        n=n-j;
                        cnt=cnt+1;
                        j=j+2;         
                 }
                 else{
                         break;
                 }
                }       
        printf("%d\n",cnt);
        }
                
return 0;
}
